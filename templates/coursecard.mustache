{{!
    @template block_my_day_buttons/coursecard

    This template renders a course for the my_day_buttons block.

    Example context (json):
    {
        "courses": [
            {
                "id" => "18",
                "fullname" => "Test Meta Course",
                "viewurl" => "https://moodlesite/course/view.php?id=18",
                "courseimageurl" => "data:image/svg+xml;base64,",
                "courseimagetokenised" => "data:image/svg+xml;base64,",
            }
        ]
    }
}}
<div data-courseid="{{id}}" class="course">
    <a href="{{viewurl}}">
        <span class="courseimage" style="background-image: url('{{courseimageurl}}');">
            <span class="sr-only">Course image</span>
        </span>
        <span class="course-info">
            <span class="sr-only">Course name</span>
            <span class="text-truncate">{{fullname}}</span>
        </span>
    </a>
</div>